{
  "name": "Yahoo! Finance",
  "description": "Get market price of specified stock",
  "category": "Finance",
  "url": "https://query2.finance.yahoo.com/v8/finance/chart/{ticker}",
  "thumbnail": "https://finance.yahoo.com/favicon.ico",
  "homepage": "https://finance.yahoo.com",
  "options": [
    {
      "name": "ticker",
      "description": "Ticker symbol of the stock",
      "example": "AAPL"
    }
  ],
  "items": {
    "extractor": "json",
    "title": [
      "$.chart.result[0].meta.longName",
      " (",
      "$.chart.result[0].meta.symbol",
      ")"
    ],
    "subtitle": [
      "$.chart.result[0].meta.regularMarketPrice",
      " ",
      "$.chart.result[0].meta.currency"
    ],
    "leading": {
      "top": "",
      "bottom": ""
    },
    "trailing": {
      "top": "",
      "bottom": ""
    },
    "image": "",
    "notes": [],
    "url": "https://finance.yahoo.com/quote/{ticker}/"
  }
}
